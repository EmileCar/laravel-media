{
    "name": "carone/laravel-media",
    "description": "Adds media functionality to laravel apps",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.35",
        "intervention/image-laravel": "^1.5",
        "lorisleiva/laravel-actions": "^2.9",
        "illuminate/database": "^12.35",
        "laravel/framework": "^12.35"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.6",
        "illuminate/http": "^12.35"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carone\\Media\\CaroneMediaServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Carone\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carone\\Media\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "authors": [
        {
            "name": "Caron Emile"
        }
    ]
}
